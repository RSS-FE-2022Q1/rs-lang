.pages-list {
  display: flex;
  gap: 0.25rem;

  li {
    @apply border-b-2 rounded bg-yellow-300 w-6 text-center;
    cursor: pointer;

    a{
      @apply inline-block;
      width: 100%;
    }

  }

  .pagelink-done{
    @apply bg-green-300;
  }

  .pagelink-active{
    @apply bg-violet-300;
  }
}

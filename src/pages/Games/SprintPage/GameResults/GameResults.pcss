.results {
    @apply w-8/12 flex flex-col items-center;
}

.results_table{ 
    @apply flex w-full justify-between; 

    & .results_list{ 
        @apply flex flex-col w-full text-center;
    }

    & .results_wrong {
        @apply bg-red-100;
    }

    & .results_correct {
        @apply bg-green-100;
    }
}
.button_wrapper{
    @apply inline-flex rounded-md shadow mx-4;

    & .cta_button{
        @apply inline-flex items-center justify-center px-5 py-3 border border-transparent text-base font-medium rounded-md ;
    }

    & .button_primary{
       @apply text-white bg-indigo-600 hover:bg-indigo-700;
    }

    & .button_secondary{
       @apply text-indigo-600 bg-white hover:bg-indigo-50
    }

}


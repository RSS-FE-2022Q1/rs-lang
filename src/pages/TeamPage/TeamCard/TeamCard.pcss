.team_card{
    @apply w-3/12 py-8 px-4  flex flex-col items-center bg-gray-50 ;


    & .card_photo {
       @apply w-9/12;
    }

    & .card_photoImg {
        @apply max-w-full rounded-full ;
        height: auto;
    }

    & .card_name {
        @apply my-2;
    }

    & .card_role {
        @apply my-2 text-indigo-500 font-semibold	;
    }

    & .card_description {
        @apply text-center;
    }

    & .card_links {
        @apply flex;
    }
}